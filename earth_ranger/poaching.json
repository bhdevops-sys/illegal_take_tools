{
   "schema": 
   {
       "$schema": "http://json-schema.org/draft-04/schema#",
       "title": "Poaching (poaching)",
     
       "type": "object",

       "properties": 
       {
            "evidence": {
                "type": "string",
                "title": "Poaching evidence",
                 "enum": {{enum___evidence___values}},
               "enumNames": {{enum___evidence___names}}   
            },
             "area": {
                "type": "string",
                "title": "Area",
                 "enum": {{enum___area___values}},
               "enumNames": {{enum___area___names}}   
            },  
            "species": {
                "type": "string",
                "title": "Species",
                 "enum": {{enum___species___values}},
               "enumNames": {{enum___species___names}}
            }, 
            "poaching_estimated_days": {
                "type": "string",
                "title": "Estimated days since poaching",
                 "enum": {{enum___day_since_poaching___values}},
               "enumNames": {{enum___day_since_poaching___names}}
            }, 
             "poaching_sum": {
                "type": "number",
                "title": "Sum of evidence",
                "minimum": 0
            },  
             "poaching_arrestees": {
                "type": "number",
                "title": "Number of arrestees",
                "minimum": 0
            }, 
            "poaching_escapees": {
                "type": "number",
                "title": "Number of escapees",
                "minimum": 0
            }, 
            "poaching_action_taken":{
                "type": "string",
                "title": "Action taken",
                 "enum": {{enum___action_taken___values}},
               "enumNames": {{enum___action_taken___names}}
            },
             "poaching_comments":{
                "type": "string",
                "title": "Comments"
            }

       }
   },
 "definition": [
